<html lang="en">

<head>
    <base href="./">
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
    <meta name="description" content="CoreUI - Open Source Bootstrap Admin Template">
    <meta name="author" content="Łukasz Holeczek">
    <meta name="keyword" content="Bootstrap,Admin,Template,Open,Source,jQuery,CSS,HTML,RWD,Dashboard">
    <title>CoreUI Free Bootstrap Admin Template</title>
    <!-- Icons-->
    <link href="/vendors/@coreui/icons/css/coreui-icons.min.css" rel="stylesheet">
    <link href="/vendors/flag-icon-css/css/flag-icon.min.css" rel="stylesheet">
    <link href="/vendors/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <link href="/vendors/simple-line-icons/css/simple-line-icons.css" rel="stylesheet">
    <!-- Main styles for this application-->
    <link href="/css/style.css" rel="stylesheet">
    <link href="/vendors/pace-progress/css/pace.min.css" rel="stylesheet">
    <link href="/css/login.css" rel="stylesheet">

</head>

<body class="app flex-row align-items-center">
    <div class="container">
        <main class="main">
            <div class="card text-white  text-center" style="background:rgba(97, 134, 201, 0.295);">
                <div class="card-body">
                    <blockquote class="card-bodyquote">

                        <div class="container-fluid">
                            <div class="row">
                                <div class="animated fadeIn col-md-8">
                                    <!-- 轮播图-->
                                    <h1 style="margin-left: 10px;color: white">账号登录</h1>
                                    <hr class="my-3" style="border:1px solid white;width: 95%;">
                                    <div class="card-body">
                                        <div class="carousel slide" id="carouselExampleIndicators" data-ride="carousel">
                                            <ol class="carousel-indicators">
                                                <li class="" data-target="#carouselExampleIndicators" data-slide-to="0">
                                                </li>
                                                <li data-target="#carouselExampleIndicators" data-slide-to="1" class="">
                                                </li>
                                                <li data-target="#carouselExampleIndicators" data-slide-to="2"
                                                    class="active"></li>
                                            </ol>
                                            <div class="carousel-inner">
                                                <div class="carousel-item">
                                                    <img class="d-block w-100" src="/images/q1.jpg"
                                                        data-holder-rendered="true">
                                                </div>
                                                <div class="carousel-item">
                                                    <img class="d-block w-100" src="/images/q2.webp"
                                                        data-holder-rendered="true">
                                                </div>
                                                <div class="carousel-item active">
                                                    <img class="d-block w-100" src="/images/q3.jpg"
                                                        data-holder-rendered="true">
                                                </div>
                                            </div>
                                            <a class="carousel-control-prev" href="#carouselExampleIndicators"
                                                role="button" data-slide="prev">
                                                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                                                <span class="sr-only">Previous</span>
                                            </a>
                                            <a class="carousel-control-next" href="#carouselExampleIndicators"
                                                role="button" data-slide="next" se_prerender_url="loading">
                                                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                                                <span class="sr-only">Next</span>
                                            </a>
                                        </div>
                                    </div>

                                </div>
                                <div class="col-md-4" style="margin-top:50px">
                                    <div class="card-group">
                                        <div class="card">
                                            <div class="card-body" style="margin-top: 20px">
                                                <div class="mb-4 col-md-12" style="margin-left: 125px;margin-top: 35px">
                                                </div>
                                                <div class="input-group mb-4 col-md-12">
                                                    <div class="input-group-prepend">
                                                        <span class="input-group-text">
                                                            <i class="fa fa-user"></i>
                                                        </span>
                                                    </div>
                                                    <input autofocus class="form-control" placeholder="账号"
                                                        name="username" id="username" required type="text">
                                                </div>

                                                <div class="input-group mb-4 col-md-12">
                                                    <div class="input-group-prepend">
                                                        <span class="input-group-text">
                                                            <i class="fa fa-lock"></i>
                                                        </span>
                                                    </div>
                                                    <input class="form-control" placeholder="密码" name="password"
                                                        id="password" required type="password">
                                                </div>

                                                <div class="form-group row mb-3 col-md-12">
                                                    <div class="col-md-12 col-form-label">
                                                        <div class="form-check form-check-inline mr-1 col-md-6 "
                                                            style="margin-left: 15px">
                                                            <input class="form-check-input" id="inline-radio1"
                                                                name="roles" type="radio" value="超级管理员">
                                                            <label class="form-check-label"
                                                                for="inline-radio1">超级管理员</label>
                                                        </div>
                                                        <div class="form-check form-check-inline mr-1">
                                                            <input class="form-check-input" id="inline-radio2"
                                                                name="roles" type="radio" value="销售员">
                                                            <label class="form-check-label"
                                                                for="inline-radio2">销售员</label>
                                                        </div>
                                                        <div class="form-check form-check-inline mr-1"
                                                            style="margin-left: 13px;margin-top: 10px">
                                                            <input class="form-check-input" id="inline-radio3"
                                                                name="roles" type="radio" value="财务">
                                                            <label class="form-check-label"
                                                                for="inline-radio3">财务</label>
                                                        </div>
                                                        <div class="form-check form-check-inline mr-1"
                                                            style="margin-left: 60px">
                                                            <input class="form-check-input" id="inline-radio4"
                                                                name="roles" type="radio" value="跟单员">
                                                            <label class="form-check-label"
                                                                for="inline-radio4">跟单员</label>
                                                        </div>

                                                    </div>
                                                </div>


                                                <div class="row" style="margin-left: 15px">
                                                    <div class="col-md-11">
                                                        <button href="" class="btn btn-block btn-info"
                                                            onclick="login()" >登录</button>
                                                    </div>
                                                </div>

                                                <div class="row" style="margin-left: 95px">

                                                    <div class="col-4 text-right">
                                                        <a class="btn btn-link px-0" href="#"
                                                            style="color:white;">忘记密码</a>
                                                    </div>
                                                </div>

                                            </div>
                                        </div>
                                    </div>


                                </div>
                            </div>
                        </div>
                        </footer>
                    </blockquote>
                </div>
            </div>
        </main>
    </div>
    <script src="/vendors/jquery/js/jquery.min.js"></script>
    <script src="/vendors/popper.js/js/popper.min.js"></script>
    <script src="/vendors/bootstrap/js/bootstrap.min.js"></script>
    <script src="/vendors/pace-progress/js/pace.min.js"></script>
    <script src="/vendors/perfect-scrollbar/js/perfect-scrollbar.min.js"></script>
    <script src="/vendors/@coreui/coreui/js/coreui.min.js"></script>
    <script type="text/javascript" src="/javascripts/login.js"></script>

</body>

</html>