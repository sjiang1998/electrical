                            <div class="modal-dialog modal-success modal-dialog-centered" role="document">
                                <div class="modal-content">
                                    <div class="modal-header">
                                        <h4 class="modal-title">提示</h4>
                                        <button class="close" type="button" data-dismiss="modal" aria-label="Close">
                                            <span aria-hidden="true">×</span>
                                        </button>
                                    </div>
                                    <div class="modal-body">
                                        <b>
                                            <span>删除成功！</span>
                                        </b>
                                    </div>
                                    <div class="modal-footer">
                                        <button class="btn btn-secondary" type="button"
                                            onclick="refreshwin()">确认</button>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <!-- Model新增 -->
                        <div class="modal fade show" id="a1" tabindex="-1" role="dialog" aria-labelledby="myModalLabel"
                            style="display:none;" aria-hidden="true">
                            <div class="modal-dialog modal-primary" role="document">
                                <div class="modal-content">
                                    <div class="modal-header">
                                        <h4 class="modal-title">新增</h4>
                                        <button class="close" type="button" data-dismiss="modal" aria-label="Close">
                                            <span aria-hidden="true">×</span>
                                        </button>
                                    </div>
                                    <div class="card-body">

                                        <form class="form-horizontal needs-validation" novalidate>
                                            <div class="form-group row">
                                                <label class="col-md-3 col-form-label">用户名</label>
                                                <div class="col-md-9">
                                                    <input class="form-control" type="text" id="addName"
                                                        pattern="^[\u4E00-\u9FA5]{0,5}$" required>
