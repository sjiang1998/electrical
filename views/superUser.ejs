                                                        <table>
                                                            <tbody>
                                                                <tr>
                                                                    <th>用户姓名</th>
                                                                    <th><%= users[i].username%></th>
                                                                </tr>
                                                                <tr>
                                                                    <th>用户身份</th>
                                                                    <th><%= users[i].role%></th>
                                                                </tr>
                                                                <tr>
                                                                    <th>注册时间</th>
