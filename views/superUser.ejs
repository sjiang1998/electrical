                                    </div>
                                    <div v-cloak class="card-body collapse show " id="collapseExample">
                                        <div class="animated fadeIn">
                                            <div class="row" id="r1">

                                                <% for(var i = 0;i < users.length;i++){ %>
                                                <% if(users[i].role == "跟单员"){%>
                                                <div class="col-sm-6 col-lg-3 box">
                                                    <div class="card" >
                                                        <i class="fa fa-user-circle-o fa-lg mt-3 i1">
                                                            <span class="s1">用户名</span>
                                                        </i>
                                                        <hr>
                                                        <table>
                                                            <tbody>
                                                                <tr>
                                                                    <th>用户姓名</th>
                                                                    <th><%= users[i].username%></th>
                                                                </tr>
                                                                <tr>
                                                                    <th>用户身份</th>
                                                                    <th><%= users[i].role%></th>
                                                                </tr>
                                                                <tr>
                                                                    <th>注册时间</th>
                                                                    <th class="time-h"><%= date[i]%></th>
                                                                </tr>
                                                                <tr>
                                                                    <th>用户状态</th>
                                                                    <th>
                                                                        <% if(users[i].status == 1){%>
                                                                        <label
                                                                            class="switch switch-label switch-pill switch-success"
                                                                            style="padding-top:8px">
                                                                            <input class="switch-input" type="checkbox"
                                                                                checked
                                                                                onclick="changeStatus('<%= users[i]._id%>')">
                                                                            <span class="switch-slider"
                                                                                data-checked="On"
                                                                                data-unchecked="Off"></span>
                                                                        </label>
                                                                        <% }else{ %>
                                                                        <label
                                                                            class="switch switch-label switch-pill switch-success"
                                                                            style="padding-top:8px">
                                                                            <input class="switch-input" type="checkbox"
                                                                                selected
                                                                                onclick="changeStatus('<%= users[i]._id%>')">
                                                                            <span class="switch-slider"
                                                                                data-checked="On"
                                                                                data-unchecked="Off"></span>
                                                                        </label>
                                                                        <% } %>

                                                                    </th>
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                        <div class="operate">
