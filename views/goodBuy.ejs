                <a class="nav-link" href="/login">
                    <i class="fa fa-user-circle fa-2x mt-2">
                        <h6>登录</h6>
                    </i>
                </a>
            </li>
        </ul>

        <% } %>


        <button class="navbar-toggler aside-menu-toggler d-lg-none" type="button" data-toggle="aside-menu-show">
            <span class="navbar-toggler-icon"></span>
        </button>
    </header>


    <div class="app-body">

        <main class="main">

            <ol class="breadcrumb">
                <% if(factories !=null){%>

                <div class="col">
                    <ul class="nav nav-pills mb-1" id="pills-tab" role="tablist">
                        <li class="nav-item fen">
                            <a class="nav-link active show" id="pills-home-tab" data-toggle="pill" href="#pills-home"
                                role="tab" aria-controls="pills-home" aria-selected="false">工厂表单</a>
                        </li>
                        <li class="nav-item  fen-lei">
                            <a class="nav-link " id="pills-bought-tab" data-toggle="pill" href="#pills-bought "
