                            <div class="animated fadeIn">
                                <div class="card">
                                    <div class="card-header">
                                        <div class="input-prepend input-group col-sm-10">
                                            <div class="input-group-prepend">
                                                <span class="input-group-text">
                                                    <i class="fa fa-search"></i></span>
                                            </div>


                                        </div>
                                    </div>
                                    <div v-cloak class="card-body collapse show " id="collapseExample">
                                        <table class="table table-responsive-sm table-bordered" v-if="items.length!=0">
                                            <thead>
                                                <tr>
                                                    <th><input type="checkbox" id="check_all" /></th>
                                                    <th>商品名</th>
                                                    <th>工厂名</th>
                                                    <th>生产地址</th>
                                                    <th>成本</th>
                                                    <th>派送地址</th>
                                                    <th>商品数量</th>
                                                    <th>操作</th>

                                                </tr>
                                            </thead>
                                            <tbody>

                                                <% for(var i=0;i < factories.length;i++){%>
                                                <tr>
                                                    <td><input type="checkbox" class="check_item" />#<%= i+1 %></td>
                                                    <td><%= factories[i].gname%></td>
                                                    <td><%= factories[i].fname%></td>
                                                    <td><%= factories[i].faddress%></td>
                                                    <td><%= factories[i].fcost%></td>
                                                    <td><%= order.caddress%></td>
                                                    <td><%= order.goodnum%></td>

                                                    <td>
                                                        <button class="btn btn-block btn-ghost-info col-6"
                                                            data-toggle="modal" data-target="#b1"
                                                            onclick="finalbuy('<%= factories[i].fcost%>','<%= factories[i].faddress%>')">
                                                            采购</button>

                                                </tr>
                                                <% } %>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>

                            <!-- Modal采购 -->
                            <div class="modal fade" id="b1" data-backdrop="static" tabindex="-1" role="dialog"
                                aria-labelledby="myModalLabel" aria-hidden="true">
                                <div class="modal-dialog">
